{
    "presets": [
        ["env", {
            "modules": false,
            "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }],
        "stage-2", "es2015"
    ],
    "plugins": [
        ["component", [{
            "libraryName": "element-ui",
            "styleLibraryName": "theme-default",
            "transform-runtime": "transform-runtime"
        }]]
    ],
    "env": {
        "test": {
            "presets": ["env", "stage-2", "es2015"],
            "plugins": ["istanbul"]
        }
    }
}